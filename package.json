{
  "name": "@kryter/barnstorm",
  "version": "2.0.0",
  "description": "Organize end to end test code",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**' && npx prettier --check 'src/**'",
    "lint:fix": "eslint --fix 'src/**'",
    "format": "prettier --write 'src/**'",
    "precommit": "tsc && eslint --fix 'src/**' && prettier --write 'src/**' && jest",
    "test": "jest"
  },
  "files": [
    "docs",
    "lib",
    "src"
  ],
  "directories": {
    "docs": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kryter/barnstorm.git"
  },
  "keywords": [
    "automation",
    "barnstorm",
    "browser",
    "e2e",
    "end-to-end",
    "integration",
    "test",
    "testing"
  ],
  "author": "Kori Ryter <kryter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kryter/barnstorm/issues"
  },
  "home": "https://github.com/kryter/barnstorm#readme",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^27.4.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "rxjs": "^7.5.1"
  }
}
