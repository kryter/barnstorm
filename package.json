{
  "name": "@kryter/barnstorm",
  "version": "1.0.0",
  "description": "Organize end to end test code",
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**' && npx prettier --check .",
    "lint:fix": "eslint --fix 'src/**'",
    "format": "prettier --write 'src/**'",
    "precommit": "tsc && eslint --fix 'src/**' && prettier --write 'src/**' && jest",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kryter/barnstorm.git"
  },
  "keywords": [
    "automation",
    "barnstorm",
    "browser",
    "e2e",
    "end-to-end",
    "integration",
    "test",
    "testing"
  ],
  "author": "Kori Ryter <kryter@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kryter/barnstorm/issues"
  },
  "homepage": "https://github.com/kryter/barnstorm#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.2"
  }
}
